                            {{form_row(coursPhoneForm.submit, {'label_col':3,'widget_col': 2})}}
                        {{form_end(coursPhoneForm)}}
                    </div>
                </div>
                {%endif%}
                {%if cours.status is not null and cours.status is not constant('STATUS_PLANNED', cours)%}
                <br>
                <h3 class="legend">{{'Cours.tlabel.coursEditStatus'|trans}}</h3>
                <div class="panel panel-danger">
                    <div class="panel-body">
                        {{form_start(coursStatusForm, {'method':'POST', 'action':
                        path('Teacher__cours_edit_post', {'id': cours.id}), 'style': 'horizontal'})}}
                            {{form_row(coursStatusForm.status, {'label_col': 3, 'widget_col': 2})}}
                            {{form_row(coursStatusForm.submit, {'label_col':3,'widget_col': 2})}}
                        {{form_end(coursStatusForm)}}
                    </div>
                </div>
                {%endif%}
                {%if cours.status is constant('STATUS_DONE', cours)%}
                <br>
                <h3 class="legend">{{'Cours.tlabel.coursEditCorrection'|trans}}</h3>
                <div class="panel panel-danger">
                    <div class="panel-body">
                        {{form_start(coursCorrectionForm, {'method':'POST', 'action':
                        path('Teacher__cours_edit_post', {'id': cours.id}), 'style': 'horizontal'})}}
                            {{form_row(coursCorrectionForm.progress, {'label_col': 3, 'widget_col': 9,
