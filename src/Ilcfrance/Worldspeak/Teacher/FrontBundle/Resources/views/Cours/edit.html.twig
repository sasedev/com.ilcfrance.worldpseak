                                </tr>
                                {%endfor%}
                            </tbody>
                        </table>
                        {%endif%}
                     </div>
                </div>
                <br>
                {%endif%}
                {%endfor%}
            </div>
            {%if timeCredit.lockout is not constant('LOCKOUT_LOCKED', timeCredit)
                and cours.teacher is not null
                and app.user.id==cours.teacher.id%}
            <div class="tab-pane fade{%if tabActive==2%} in active{%endif%}" id="edit">
                <br>
                <h3 class="legend">{{'Cours.tlabel.coursEditType'|trans}}</h3>
                <div class="panel panel-danger">
                    <div class="panel-body">
                        {{form_start(coursTypeForm, {'method':'POST', 'action':
                        path('Teacher__cours_edit_post', {'id': cours.id}), 'style': 'horizontal'})}}
                            {{form_row(coursTypeForm.type, {'label_col': 3, 'widget_col': 2})}}
                            {{form_row(coursTypeForm.submit, {'label_col':3,'widget_col': 2})}}
                        {{form_end(coursTypeForm)}}
                    </div>
                </div>
                {%if cours.phone is empty%}
                <br>
                <h3 class="legend">{{'Cours.tlabel.coursEditPhone'|trans}}</h3>
