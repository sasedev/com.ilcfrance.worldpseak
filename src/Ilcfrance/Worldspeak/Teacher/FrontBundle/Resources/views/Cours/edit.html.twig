                                {%else%}
                                    {{label_danger('_NA'|trans)}}
                                {%endif%}
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-lg-3 text-right">{{'Cours.dtCrea'|trans|raw}} :</div>
                            <div class="col-lg-9">{{cours.dtCrea|localizeddate('full', 'none', app.request.locale )|capitalize}} {{'_at'|trans}}
