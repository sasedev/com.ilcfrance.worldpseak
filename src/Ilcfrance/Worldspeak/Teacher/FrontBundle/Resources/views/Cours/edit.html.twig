{%extends 'IlcfranceWorldspeakTeacherFrontBundle::layout.html.twig'%}
{%set timeCredit = cours.timeCredit%}
{%set trainee = timeCredit.trainee%}
{%block breadcrumb%}
<ol class="breadcrumb">
    <li><a href="{{path('Teacher__default_homepage')}}">{{icon('home')}} &nbsp; {{'breadcrumb.homepage'|trans}}</a></li>
    <li><a href="{{path('Teacher__trainee_edit_get', {'id': timeCredit.trainee.id})}}">{{icon('edit')}} &nbsp;
            {{'breadcrumb.trainee.edit'|trans({'%trainee%': timeCredit.trainee.fullname})|raw}}</b>"
    </a></li>
    <li><a href="{{path('Teacher__timeCredit_edit_get', {'id': timeCredit.id})}}">{{icon('time')}} &nbsp;
            {{'breadcrumb.timeCredit.edit'|trans({'%totalHours%':timeCredit.totalHours,'%ftype%':
            ('TimeCredit.ftype.'~timeCredit.ftype)|trans,'%level%': ('TimeCredit.level.'~timeCredit.level)|trans})|raw}}</a>
    </li>
    <li class="active"><a href="{{path('Teacher__cours_edit_get', {'id': cours.id})}}">{{icon('earphone')}} &nbsp;
            {{'breadcrumb.cours.edit'|trans({'%dStart%':cours.dtStart|localizeddate('long', 'none', app.request.locale )|capitalize,
            '%tStart%': cours.dtStart|localizeddate('none', 'medium', app.request.locale ),
            '%dtEnd%': cours.dtEnd|localizeddate('long', 'none', app.request.locale)})|raw }}</a>
    </li>
</ol>
{%endblock breadcrumb%}
{%block pagebody_content%}
<div class="row">
    <div class="col-lg-12">
        <ul class="nav nav-tabs">
            {%if tabActive is not defined%}{%set tabActive=1%}{%endif%}
            <li {%if tabActive==1%} class="active"{%endif%}><a href="#preview" data-toggle="tab">{{'Cours.tab.preview'|trans|raw}}</a></li>
            {%if timeCredit.lockout is not constant('LOCKOUT_LOCKED', timeCredit)
                and cours.teacher is not null
                and app.user.id==cours.teacher.id%}
            <li {%if tabActive==2%} class="active"{%endif%}><a href="#edit" data-toggle="tab">{{'Cours.tab.edit'|trans|raw}}</a></li>
            {%endif%}
            {%if cours.documents|length != 0%}
            <li {%if tabActive==3%} class="active"{%endif%}><a href="#coursDocument" data-toggle="tab">{{'Cours.tab.CoursDocument'|trans|raw}}</a></li>
            {%endif%}
            <li {%if tabActive==4%} class="active"{%endif%}><a href="#timeCreditDocument" data-toggle="tab">{{'Cours.tab.TimeCreditDocument'|trans|raw}}</a></li>
            <li {%if tabActive==5%} class="active"{%endif%}><a href="#timeCredit" data-toggle="tab">{{'Cours.tab.TimeCredit'|trans|raw}}</a></li>
            <li {%if tabActive==6%} class="active"{%endif%}><a href="#trainee" data-toggle="tab">{{'Cours.tab.Trainee'|trans|raw}}</a></li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane fade{%if tabActive==1%} in active{%endif%}" id="preview">
                <br>
                <h3 class="legend">{{'Cours.tlabel.preview'|trans}}</h3>
                <div class="panel panel-info">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-3 text-right">{{'Cours.trainee'|trans|raw}} :</div>
                            <div class="col-lg-9">
                                <a href="{{path('Teacher__trainee_edit_get', {'id': timeCredit.trainee.id})}}"><i class="fa fa-user"></i> {{timeCredit.trainee.fullname}}</a>
                            </div>
                        </div>
                        <br>
                        <div class="row">

                            <div class="col-lg-3 text-right">{{'Cours.teacher'|trans|raw}} :</div>
                            <div class="col-lg-9">
                                {%if cours.teacher is not null%}
                                <b>{{icon('user')}} {{cours.teacher.fullname}}</b>
                                {%else%}
                                {{label_danger('_NA'|trans)}}
                                {%endif%}
                            </div>
                        </div>
                        <br>
                        <div class="row">

                            <div class="col-lg-3 text-right">{{'Cours.dtStart'|trans|raw}} :</div>
                            <div class="col-lg-9">{{cours.dtStart|localizeddate('full', 'none', app.request.locale )|capitalize}} {{'_at'|trans}}
                            {{cours.dtStart|localizeddate('long', 'none', app.request.locale)}}</div>
                        </div>
                        <br>
                        <div class="row">

                            <div class="col-lg-3 text-right">{{'Cours.dtEnd'|trans|raw}} :</div>
                            <div class="col-lg-9">{{cours.dtEnd|localizeddate('full', 'none', app.request.locale )|capitalize}} {{'_at'|trans}}
                            {{cours.dtEnd|localizeddate('long', 'none', app.request.locale)}}</div>
                        </div>
                        <br>
                        <div class="row">

                            <div class="col-lg-3 text-right">{{'Cours.duration'|trans|raw}} :</div>
                            <div class="col-lg-9">{{cours.duration}} {{'_minutes'|trans}}</div>
                        </div>
                        <br>
                        <div class="row">

                            <div class="col-lg-3 text-right">{{'Cours.phone'|trans|raw}} :</div>
                            <div class="col-lg-9">
                                {%if cours.phone is not empty%}
                                <a href="tel:{{cours.phone}}">{{icon('earphone')}} {{cours.phone}}</a>
                                {%else%}
                                {{label_danger('_NA'|trans)}}
                                {%endif%}
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-lg-3 text-right">{{'Cours.type'|trans|raw}} :</div>
                            <div class="col-lg-9">
                                {%if cours.type is not null and cours.type is not constant('TYPE_UNDEFINED', cours)%}
                                    {{('Cours.type.'~cours.type)|trans}}
                                {%else%}
                                    {{label_danger(('Cours.type.'~constant('TYPE_UNDEFINED', cours))|trans)}}
                                {%endif%}
                            </div>
                        </div>
                        <br>
                        <div class="row">
                            <div class="col-lg-3 text-right">{{'Cours.status'|trans|raw}} :</div>
                            <div class="col-lg-9">
                                {%if cours.status is not null
                                    and cours.status is not constant('STATUS_PLANNED_PENDING', cours)%}
                                    {{('Cours.status.'~cours.status)|trans}}
                                {%elseif cours.status is constant('STATUS_PLANNED_PENDING', cours)%}
